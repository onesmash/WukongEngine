# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug_osx
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_osx)
  uv_config = debug_osx
endif
ifeq ($(config),release_osx)
  uv_config = release_osx
endif

PROJECTS := uv

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

uv:
ifneq (,$(uv_config))
	@echo "==== Building uv ($(uv_config)) ===="
	@${MAKE} --no-print-directory -C . -f uv.make config=$(uv_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f uv.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_osx"
	@echo "  release_osx"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   uv"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"